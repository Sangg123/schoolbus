import api from './api'

const getAllDriver = async (id, userId, citizenId, licenseId) => {
    var response = null;
    try {
        response = await api.get("/dirver", {id, userId, citizenId, licenseId});
        return response;
    } catch (err) {
        console.error(err);
        throw err;
    }
};

export default getAllDriver;